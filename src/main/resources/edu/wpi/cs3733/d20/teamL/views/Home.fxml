<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane style="-fx-background-color: #0d2e57;" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.wpi.cs3733.d20.teamL.views.controllers.NavigationController">
    <children>
        <ImageView fx:id="iHome" fitHeight="720.0" fitWidth="1280.0" opacity="0.46" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <image>
                <Image url="@../assets/Home-image.jpg"/>
            </image>
        </ImageView>
        <GridPane prefHeight="687.2" prefWidth="1080.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="110.60001373291016" minHeight="10.0" prefHeight="35.400001525878906"
                                vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="570.133321762085" minHeight="10.0" prefHeight="513.8666137695312"
                                vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="233.5999755859375" minHeight="0.0" prefHeight="130.7333862304688"
                                vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <VBox alignment="CENTER" prefHeight="687.0" prefWidth="1080.0" GridPane.rowIndex="1">
                    <children>
                        <Label alignment="CENTER" prefHeight="100.0" prefWidth="598.0" text="Where do you want to go?"
                               textFill="WHITE">
                            <font>
                                <Font name="System Bold" size="48.0"/>
                            </font>
                        </Label>
                        <HBox alignment="CENTER">
                            <children>
                                <JFXTextField fx:id="searchBox" focusColor="#0d2e57" maxWidth="538.0"
                                              onKeyTyped="#inputHandler" prefHeight="63.0" prefWidth="538.0"
                                              promptText="Ex. &quot;Pharmacy&quot; or &quot;Restroom&quot;"
                                              style="-fx-background-color: ffffff;" unFocusColor="WHITE"
                                              viewOrder="20.0">
                                    <font>
                                        <Font size="30.0"/>
                                    </font>
                                    <padding>
                                        <Insets left="20.0"/>
                                    </padding>
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                </JFXTextField>
                                <JFXButton prefHeight="66.0" prefWidth="70.0" style="-fx-background-color: #0d2857;"
                                           text=" " fx:id="btnSearch" onAction="#searchMap">
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                    <graphic>
                                        <ImageView fitHeight="47.0" fitWidth="46.0">
                                            <image>
                                                <Image url="@../assets/Search-button-home.png"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </JFXButton>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <HBox alignment="CENTER" prefHeight="126.0" prefWidth="1080.0" spacing="81.0"
                      style="-fx-background-color: #0d2e57;" GridPane.halignment="CENTER" GridPane.rowIndex="2"
                      GridPane.valignment="CENTER">
                    <children>
                        <JFXButton fx:id="btnLogin" onAction="#handleButtonAction" prefHeight="68.0" prefWidth="232.0"
                                   style="-fx-background-color: #0d2e57; -fx-background-radius: 50; -fx-border-color: FFFFFF; -fx-border-radius: 50; -fx-border-width: 8;"
                                   text=" LOGIN" textFill="WHITE">
                            <font>
                                <Font name="System Bold" size="32.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="48.0" fitWidth="52.0">
                                    <image>
                                        <Image url="@../assets/outline_people_alt_white_48dp.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </JFXButton>
                        <JFXButton fx:id="btnMap" alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT"
                                   onAction="#handleButtonAction" prefHeight="107.0" prefWidth="245.0"
                                   style="-fx-background-color: #0d2e57; -fx-background-radius: 50; -fx-border-radius: 50; -fx-border-color: ffffff; -fx-border-width: 8;"
                                   text=" MAP" textFill="WHITE">
                            <font>
                                <Font name="System Bold" size="40.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="59.0" fitWidth="58.0">
                                    <image>
                                        <Image url="@../assets/baseline_explore_white_48dp.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                        </JFXButton>
                        <JFXButton fx:id="btnServices" onAction="#handleButtonAction" prefHeight="61.0"
                                   prefWidth="279.0"
                                   style="-fx-background-color: #0d2e57; -fx-background-radius: 50; -fx-border-radius: 50; -fx-border-color: FFFFFF; -fx-border-width: 8;"
                                   text=" SERVICES" textFill="WHITE">
                            <font>
                                <Font name="System Bold" size="32.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="43.0" fitWidth="44.0">
                                    <image>
                                        <Image url="@../assets/baseline_room_service_white_48dp.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </JFXButton>
                    </children>
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                    <padding>
                        <Insets left="25.0"/>
                    </padding>
                </HBox>
                <JFXButton fx:id="btnHelp" onAction="#handleButtonAction" prefHeight="70.0" prefWidth="214.0"
                           style="-fx-background-color: #d00808;" text="EMERGENCY" textFill="WHITE" underline="true"
                           GridPane.halignment="RIGHT" GridPane.valignment="TOP">
                    <font>
                        <Font name="System Bold" size="28.0"/>
                    </font>
                </JFXButton>
                <Label fx:id="timeLabel" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="48.0"/>
                    </font>
                    <padding>
                        <Insets left="10.0" top="10.0"/>
                    </padding>
                </Label>
            </children>
        </GridPane>
    </children>

</AnchorPane>
